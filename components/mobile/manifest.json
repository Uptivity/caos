{
  "name": "CAOS CRM Mobile",
  "short_name": "CAOS CRM",
  "description": "Customer Relationship Management System - Mobile Interface",
  "start_url": "./Mobile.html",
  "display": "standalone",
  "orientation": "portrait-primary",
  "theme_color": "#2563eb",
  "background_color": "#ffffff",
  "scope": "./",
  "lang": "en-US",
  "categories": ["business", "productivity", "finance"],
  "icons": [
    {
      "src": "data:image/svg+xml;base64,",
      "sizes": "192x192",
      "type": "image/svg+xml",
      "purpose": "any maskable"
    },
    {
      "src": "data:image/svg+xml;base64,",
      "sizes": "512x512",
      "type": "image/svg+xml",
      "purpose": "any maskable"
    }
  ],
  "screenshots": [
    {
      "src": "data:image/svg+xml;base64,",
      "sizes": "375x812",
      "type": "image/svg+xml",
      "form_factor": "narrow",
      "label": "CAOS CRM Mobile Dashboard"
    }
  ],
  "shortcuts": [
    {
      "name": "Dashboard",
      "short_name": "Dashboard",
      "description": "Quick access to your CRM dashboard",
      "url": "./Mobile.html#dashboard",
      "icons": [
        {
          "src": "data:image/svg+xml;base64,",
          "sizes": "96x96"
        }
      ]
    },
    {
      "name": "New Lead",
      "short_name": "Add Lead",
      "description": "Create a new lead",
      "url": "./Mobile.html#leads/new",
      "icons": [
        {
          "src": "data:image/svg+xml;base64,",
          "sizes": "96x96"
        }
      ]
    },
    {
      "name": "Tasks",
      "short_name": "Tasks",
      "description": "View your tasks",
      "url": "./Mobile.html#tasks",
      "icons": [
        {
          "src": "data:image/svg+xml;base64,",
          "sizes": "96x96"
        }
      ]
    }
  ],
  "related_applications": [],
  "prefer_related_applications": false,
  "share_target": {
    "action": "./Mobile.html#share",
    "method": "POST",
    "enctype": "multipart/form-data",
    "params": {
      "title": "title",
      "text": "text",
      "url": "url",
      "files": [
        {
          "name": "files",
          "accept": ["image/*", "text/*", ".pdf", ".doc", ".docx"]
        }
      ]
    }
  },
  "protocol_handlers": [
    {
      "protocol": "mailto",
      "url": "./Mobile.html#email/compose?to=%s"
    },
    {
      "protocol": "tel",
      "url": "./Mobile.html#leads/call?phone=%s"
    }
  ],
  "file_handlers": [
    {
      "action": "./Mobile.html#import",
      "accept": {
        "text/csv": [".csv"],
        "application/json": [".json"],
        "text/vcard": [".vcf"],
        "application/vnd.ms-excel": [".xls", ".xlsx"]
      }
    }
  ],
  "edge_side_panel": {
    "preferred_width": 400
  },
  "launch_handler": {
    "client_mode": ["focus-existing", "auto"]
  }
}